{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dffromblobtogen2"
		},
		"ls_forgen2quality_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_forgen2quality'"
		},
		"ls_forhotblob_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_forhotblob'"
		},
		"ls_forhotgen2_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_forhotgen2'"
		},
		"ls_forgen2quality_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2forquality.dfs.core.windows.net/"
		},
		"ls_forhotgen2_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://gen2forquality.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dffromblobtogen2_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_forgen2quality_accountKey": {
						"value": "[parameters('ls_forgen2quality_accountKey')]"
					},
					"ls_forhotblob_connectionString": {
						"value": "[parameters('ls_forhotblob_connectionString')]"
					},
					"ls_forhotgen2_accountKey": {
						"value": "[parameters('ls_forhotgen2_accountKey')]"
					},
					"ls_forgen2quality_properties_typeProperties_url": {
						"value": "[parameters('ls_forgen2quality_properties_typeProperties_url')]"
					},
					"ls_forhotgen2_properties_typeProperties_url": {
						"value": "[parameters('ls_forhotgen2_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}